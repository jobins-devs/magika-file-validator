{
    "name": "sandip-jobin/magika-file-validator",
    "description": "Custom file type validation for Laravel using magika",
    "version": "1.0.0",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sandip Shrestha",
            "email": "sandipshrestha@jobins.jp"
        }
    ],
    "keywords": [
        "jobins",
        "magika-file-validator",
        "magika"
    ],
    "homepage": "https://github.com/sandip-jobin/magika-file-validator",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.43"
    },
    "autoload": {
        "psr-4": {
            "Jobins\\MagikaFileValidator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jobins\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "post-install-cmd": [
            "Jobins\\MagikaFileValidator\\MagikaFileValidatorServiceProvider::postInstall"
        ],
        "post-update-cmd": [
            "Jobins\\MagikaFileValidator\\MagikaFileValidatorServiceProvider::postInstall"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jobins\\MagikaFileValidator\\MagikaFileValidatorServiceProvider"
            ]
        }
    }
}
